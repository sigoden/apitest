  "version": "0.12.0",
  "bin": {
    "apitest": "dist/bin.js"
  },
  "keywords": [
    "api",
    "testing",
    "apitest",
    "ci"
  ],
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/sigoden/apitest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigoden/apitest.git"
  },
  "files": [
    "dist",
    "jslib"
  ],
  "publishConfig": {
